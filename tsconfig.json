{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es5",
		"sourceMap": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"baseUrl": "src",
		"allowJs": true,
		"jsx": "react",
		"jsxFactory": "h",
		"noImplicitAny": true,
		"lib": [
			"dom",
			"es5",
			"scripthost",
			"es6"
		]
	},
	"include": [
		"src/**/*",
		"node_modules/@types/**/*"
	],
	"files": []
}