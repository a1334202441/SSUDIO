{
	"extends": "tslint:recommended",
	"rules": {
		"arrow-parens": false,
		"interface-name": [
			false
		],
		"max-classes-per-file": [
			false
		],
		"max-line-length": [
			true,
			255
		],
		"member-access": false,
		"object-literal-sort-keys": false,
		"quotemark": false,
		"space-before-function-paren": false,
		"trailing-comma": [
			false,
			{
				"multiline": "never",
				"singleline": "never"
			}
		],
		"no-empty": false,
		"no-namespace": false,
		"no-console": [
			true,
			"log"
		],
		"ban-types": false,
		"one-line": false,
		"no-empty-interface": false,
		"semicolon": [
			false
		],
		"member-ordering": [
			false
		],
		"variable-name": [
			true,
			"allow-leading-underscore",
			"allow-snake-case"
		],
		"no-unused-expression": false,
		"radix": false,
		"no-bitwise": false,
		"forin": false,
		"one-variable-per-declaration": [
			false
		],
		"indent": [
			true,
			"spaces",
			4
		]
	}
}